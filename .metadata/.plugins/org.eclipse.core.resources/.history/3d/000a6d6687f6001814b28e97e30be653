package exercicio3;

public class Pagamento {
	
	protected double valor;
	protected boolean efetuado;
	protected FormaPagamento tipoPagamento;
	
	protected enum FormaPagamento{
		CHEQUE("Cheque"),
		DINHEIRO("Dinehiro"),
		CARTAO("Cartão");
		
		String forma;
		
		private FormaPagamento(String forma) {
			this.forma = forma;
		}
	}

}
